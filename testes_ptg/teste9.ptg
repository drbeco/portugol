// operadores logicos

inicio;
A=1.0;
B=2.0;
C=3.0;
se(A==1.0 ou B!=2.0 e C==3.0) entao
    imprima 111.1;
senao
    inicio;
    imprima C;
    se(nao (A==2 e B==3.0)) entao
        imprima 222.2;
    fim;
fim;